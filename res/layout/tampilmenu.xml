<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_height="match_parent" android:layout_width="match_parent" android:id="@+id/back">
    <LinearLayout android:orientation="horizontal" android:id="@+id/linearLayout1" android:layout_height="match_parent" android:layout_width="wrap_content">
        <LinearLayout android:layout_height="match_parent" android:id="@+id/linearLayout2" android:layout_width="wrap_content" android:layout_weight="1" android:background="#fff" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearLayout3" android:orientation="horizontal" android:gravity="center_vertical" android:layout_width="match_parent" android:background="#3498db" android:layout_height="wrap_content">
                <TextView android:id="@+id/textView1" android:layout_height="65dip" android:layout_width="65dip" android:layout_marginBottom="20dip" android:layout_marginLeft="20dip" android:layout_marginRight="10dip" android:layout_marginTop="20dip" android:background="@drawable/user"></TextView>
                <LinearLayout android:id="@+id/linearLayout6" android:layout_width="match_parent" android:gravity="center_vertical" android:orientation="vertical" android:layout_height="wrap_content">
                    <TextView android:layout_height="wrap_content" android:id="@+id/textView3" android:textSize="25dip" android:textColor="#fff" android:text="Dicky Umardhani  " android:layout_width="140dp"></TextView>
                    <TextView android:id="@+id/textView9" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Pemilik Toko" android:textSize="15dp"></TextView>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:layout_height="1dip" android:layout_width="match_parent" android:background="#ccc" android:id="@+id/linearLayout4"></LinearLayout>
            <LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout5" android:orientation="vertical" android:layout_height="match_parent">
                <LinearLayout android:layout_height="wrap_content" android:layout_width="match_parent" android:orientation="horizontal" android:padding="10dip" android:gravity="center_vertical" android:id="@+id/mberanda">
                    <TextView android:id="@+id/textView4" android:layout_marginLeft="10dip" android:layout_marginRight="5dip" android:background="@drawable/home" android:layout_height="30dp" android:layout_width="30dp"></TextView>
                    <TextView android:id="@+id/textView8" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Home" android:textColor="#000" android:textStyle="bold" android:layout_marginLeft="10dip" android:textSize="16dip"></TextView>
                </LinearLayout>
                <LinearLayout android:layout_height="1dip" android:layout_width="match_parent" android:background="#ccc" android:id="@+id/LinearLayout04" android:layout_marginLeft="5dip" android:layout_marginRight="5dip"></LinearLayout>
                <LinearLayout android:layout_height="wrap_content" android:layout_width="match_parent" android:padding="10dip" android:gravity="center_vertical" android:id="@+id/mtransaksi">
                    <TextView android:id="@+id/textView5" android:layout_marginRight="5dip" android:layout_marginLeft="10dip" android:background="@drawable/wallet" android:layout_height="30dp" android:layout_width="30dp"></TextView>
                    <TextView android:id="@+id/textView10" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textColor="#000" android:textStyle="bold" android:layout_marginLeft="10dip" android:text="Transaksi" android:textSize="16dp"></TextView>
                </LinearLayout>
                <LinearLayout android:layout_height="1dip" android:layout_width="match_parent" android:background="#ccc" android:id="@+id/LinearLayout03" android:layout_marginLeft="5dip" android:layout_marginRight="5dip"></LinearLayout>
                <LinearLayout android:layout_height="1dip" android:layout_width="match_parent" android:background="#ccc" android:id="@+id/LinearLayout01" android:layout_marginLeft="5dip" android:layout_marginRight="5dip"></LinearLayout>
                <LinearLayout android:layout_height="1dip" android:layout_width="match_parent" android:background="#ccc" android:id="@+id/LinearLayout06" android:layout_marginLeft="5dip" android:layout_marginRight="5dip"></LinearLayout>
                <LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout11" android:orientation="vertical" android:layout_height="match_parent" android:gravity="bottom">
                    <LinearLayout android:layout_height="1dip" android:layout_width="match_parent" android:background="#ccc" android:id="@+id/LinearLayout05" android:layout_marginRight="5dip" android:layout_marginLeft="5dip"></LinearLayout>
                    <LinearLayout android:layout_height="wrap_content" android:layout_width="match_parent" android:padding="10dip" android:gravity="center_vertical" android:id="@+id/mlogout">
                        <TextView android:id="@+id/TextView02" android:layout_marginLeft="10dip" android:layout_marginRight="5dip" android:background="@drawable/logout" android:layout_height="30dp" android:layout_width="30dp"></TextView>
                        <TextView android:layout_height="wrap_content" android:id="@+id/TextView01" android:layout_width="wrap_content" android:textColor="#000" android:text="Log out" android:textStyle="bold" android:layout_marginLeft="10dip"></TextView>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
