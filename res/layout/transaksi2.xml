<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="match_parent" android:background="#f7f1e3">
    <LinearLayout android:background="@drawable/wrapheader" android:layout_width="match_parent" android:id="@+id/linearLayout1" android:layout_height="55dp" android:orientation="horizontal">
        <LinearLayout android:layout_width="wrap_content" android:id="@+id/linearLayout2" android:layout_height="match_parent" android:gravity="center">
            <TextView android:layout_width="30dp" android:layout_height="30dp" android:background="@drawable/back" android:id="@+id/tpback"></TextView>
        </LinearLayout>
        <LinearLayout android:layout_width="wrap_content" android:id="@+id/linearLayout3" android:layout_height="match_parent" android:gravity="center">
            <TextView android:textSize="25dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/textView2" android:textColor="#3498db" android:layout_marginLeft="15dp" android:text="Tunai"></TextView>
        </LinearLayout>
        <LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout6" android:layout_height="match_parent" android:gravity="right">
            <TextView android:text="TextView" android:id="@+id/textView1" android:layout_width="wrap_content" android:layout_height="match_parent" android:gravity="center" android:layout_marginRight="20dp"></TextView>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
